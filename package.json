{
    "name": "dmr",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md,yaml,json,js}\""
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "eslint-config-next": "^12.0.8",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-react": "7.28.0",
        "prettier": "latest",
        "turbo": "latest"
    },
    "engines": {
        "npm": ">=7.0.0",
        "node": ">=14.0.0"
    },
    "packageManager": "yarn@1.22.10"
}
